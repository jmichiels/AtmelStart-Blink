{
  "name": "AtmelStart",
  "keywords": "AtmelStart",
  "description": "Encapsulates code generated by Atmel Start",
  "export": {
    "include": [
      "generated/*.[ch]"
    ]
  },
  "build": {
    "libArchive": false,
    "srcFilter": [
      "+<generated/hal/src/hal_io.c>",
      "+<generated/hpl/systick/hpl_systick.c>",
      "+<generated/samd21a/gcc/gcc/startup_samd21.c>",
      "+<generated/hal/utils/src/utils_syscalls.c>",
      "+<generated/hal/src/hal_delay.c>",
      "+<generated/hpl/pm/hpl_pm.c>",
      "+<generated/hpl/core/hpl_init.c>",
      "+<generated/samd21a/gcc/system_samd21.c>",
      "+<generated/hpl/core/hpl_core_m0plus_base.c>",
      "+<generated/hal/utils/src/utils_assert.c>",
      "+<generated/hpl/dmac/hpl_dmac.c>",
      "+<generated/hpl/sysctrl/hpl_sysctrl.c>",
      "+<generated/hpl/gclk/hpl_gclk.c>",
      "+<generated/hal/src/hal_init.c>",
      "+<generated/hal/utils/src/utils_list.c>",
      "+<generated/examples/driver_examples.c>",
      "+<generated/driver_init.c>",
      "+<generated/hal/src/hal_gpio.c>",
      "+<generated/hal/utils/src/utils_event.c>",
      "+<generated/hal/src/hal_sleep.c>",
      "+<generated/atmel_start.c>",
      "+<generated/hal/src/hal_atomic.oc>"
    ],
    "flags": [
      "!echo \"-Wl,-T$(pwd)/generated/samd21a/gcc/gcc/samd21e18a_flash.ld\"",
      "-Wl,--section-start=.text=0x2000",
      "-I generated",
      "-I generated/config",
      "-I generated/examples",
      "-I generated/hal/include",
      "-I generated/hal/utils/include",
      "-I generated/hpl/core",
      "-I generated/hpl/dmac",
      "-I generated/hpl/gclk",
      "-I generated/hpl/pm",
      "-I generated/hpl/port",
      "-I generated/hpl/sysctrl",
      "-I generated/hpl/systick",
      "-I generated/hri",
      "-I generated/CMSIS/Include",
      "-I generated/samd21a/include"
    ]
  }
}