{
  "name": "AtmelStart",
  "keywords": "AtmelStart",
  "description": "Encapsulates code generated by Atmel Start",
  "export": {
    "include": [
      "generated/*.[ch]"
    ]
  },
  "build": {
    "srcFilter": [
      "+<generated/hpl/usb>",
      "+<generated/hpl/usb/class/cdc/device>",
      "+<generated/hpl/usb/device>",
      "+<generated/hpl/dmac>",
      "+<generated/hal/src>",
      "+<generated/samd21a/gcc>",
      "+<generated/hpl/pm>",
      "+<generated/hpl/sysctrl>",
      "+<generated/hal/utils/src>",
      "+<generated/examples>",
      "+<generated/hpl/gclk>",
      "+<generated/samd21a/gcc/gcc>",
      "+<generated/hpl/core>",
      "+<generated/usb>"
    ],
    "flags": [
      "!echo \"-Wl,-T$(pwd)/generated/samd21a/gcc/gcc/samd21e18a_flash.ld\"",
      "-Wl,--section-start=.text=0x2000",
      "-I generated",
      "-I generated/examples",
      "-I generated/hal/include",
      "-I generated/hal/utils/include",
      "-I generated/hpl/core",
      "-I generated/hpl/dmac",
      "-I generated/hpl/gclk",
      "-I generated/hpl/pm",
      "-I generated/hpl/port",
      "-I generated/hpl/sysctrl",
      "-I generated/hpl/usb",
      "-I generated/hri",
      "-I generated/hpl/usb",
      "-I generated/config",
      "-I generated/usb",
      "-I generated/usb/class/cdc",
      "-I generated/usb/class/cdc/device",
      "-I generated/usb/device",
      "-I generated/CMSIS/Include",
      "-I generated/samd21a/include"
    ]
  }
}

